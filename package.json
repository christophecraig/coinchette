{
  "name": "coinchette-e2e",
  "version": "0.1.0",
  "description": "E2E tests for Coinchette",
  "private": true,
  "scripts": {
    "test": "playwright test",
    "test:headed": "playwright test --headed",
    "test:ui": "playwright test --ui",
